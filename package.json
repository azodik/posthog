{
  "name": "@azodik/posthog",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/azodik/posthog.git"
  },
  "description": "Posthog Config for Azodik",
  "keywords": [
    "posthog",
    "posthog-js",
    "posthog-node"
  ],
  "author": "Azodik",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prebuild": "pnpm clean",
    "prepare": "pnpm build",
    "prepublishOnly": "pnpm build",
    "changeset": "changeset",
    "version": "changeset version",
    "publish:changeset": "changeset publish",
    "publish:npm": "npm publish --access public"
  },
  "peerDependencies": {
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0"
  },
  "dependencies": {
    "posthog-js": "^1.261.4",
    "posthog-node": "^5.8.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.6",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
